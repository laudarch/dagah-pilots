<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bulbsecurity.framework.app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="4" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>

     <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" 
        android:name="com.bulbsecurity.framework.app.FrameworkAndroidApp">
        <activity
            android:name="com.bulbsecurity.framework.app.FrameworkAndroidAppActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bulbsecurity.framework.app.FunctionListActivity"></activity>
        <activity android:name="com.bulbsecurity.framework.app.RemoteAttackActivity"></activity>
        <activity android:name="com.bulbsecurity.framework.app.SocialAttackActivity"></activity>
        <activity android:name="com.bulbsecurity.framework.app.SendCommandActivity"></activity>
        <activity android:name="com.bulbsecurity.framework.app.GetAgentsActivity"></activity>
        <activity android:name="com.bulbsecurity.framework.app.GetDataActivity"></activity>
        <receiver android:name="com.bulbsecurity.framework.app.SMSReceiver">
            <intent-filter android:priority="999"><action android:name="android.provider.Telephony.SMS_RECEIVED" /></intent-filter>
        </receiver>
         <service android:name="com.bulbsecurity.framework.app.InternetPoll">
          
       </service>
        <service android:name="com.bulbsecurity.framework.app.CommandHandler">
          
       </service>
        <service android:name="com.bulbsecurity.framework.app.WebUploadService">
          
       </service>
        <service android:name="com.bulbsecurity.framework.app.WebUploadService2">
          
       </service>
          <service android:name="com.bulbsecurity.framework.app.WebUploadService3">
          
       </service> 
      <receiver android:name="com.bulbsecurity.framework.app.ServiceAutoStarter">
           <intent-filter ><action android:name="android.intent.action.BOOT_COMPLETED"></action></intent-filter> 
       </receiver>
        <service android:name="com.bulbsecurity.framework.app.InternetPoll">
          
       </service>
        <service android:name="com.bulbsecurity.framework.app.SMSService">
          
       </service>
    </application>

</manifest>
